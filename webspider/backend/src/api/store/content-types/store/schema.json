{
    "kind":  "collectionType",
    "collectionName":  "stores",
    "info":  {
                 "singularName":  "store",
                 "pluralName":  "stores",
                 "displayName":  "店铺",
                 "description":  "小红书店铺信息"
             },
    "options":  {
                    "draftAndPublish":  false
                },
    "attributes":  {
                       "name":  {
                                    "type":  "string",
                                    "required":  true
                                },
                       "externalId":  {
                                          "type":  "string",
                                          "required":  true,
                                          "unique":  true
                                      },
                       "followers":  {
                                         "type":  "biginteger",
                                         "default":  0
                                     },
                       "logoUrl":  {
                                       "type":  "string"
                                   },
                       "metadata":  {
                                        "type":  "json"
                                    },
                       "firstCollectTime": {
                           "type": "datetime"
                       },
                       "firstTotalSales": {
                           "type": "integer",
                           "default": 0
                       },
                       "latestCollectTime": {
                           "type": "datetime"
                       },
                       "latestTotalSales": {
                           "type": "integer",
                           "default": 0
                       },
                       "daysDiff": {
                           "type": "integer",
                           "default": 0
                       },
                       "salesGrowth": {
                           "type": "integer",
                           "default": 0
                       },
                       "avgDailySales": {
                           "type": "decimal",
                           "default": 0
                       },
                       "products":  {
                                        "type":  "relation",
                                        "relation":  "oneToMany",
                                        "target":  "api::product.product",
                                        "mappedBy":  "store"
                                    }
                   }
}
